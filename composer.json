{
    "name": "kerattila/laravel-track-author",
    "description": "Extension to add created_by, updated_by and deleted_by columns to Eloquent models.",
    "type": "library",
    "require": {
        "php": ">=7.4.0",
        "illuminate/config": "^10.0|^9.0|^8.0",
        "illuminate/database": "^10.0|^9.0|^8.0",
        "illuminate/support": "^10.0|^9.0|^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Keresztesi Attila",
            "email": "keresztesiattila92@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kerattila\\TrackAuthor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kerattila\\TrackAuthor\\TrackAuthorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
